<%= form_for([@restaurant, @restaurant.reservations.build]) do |f| %>
  <p>
    <%= f.label :reservee_email %><br>
    <%= f.text_field :reservee_email %>
  </p>
  <p>
    <%= f.label :dining_date %><br>
    <%= f.text_field :dining_date %>
  </p>
  <p>
    <%= f.label :note %><br>
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
